{
    "name": "spindogs/laravel-spin-forms",
    "description": "Laravel SpinForm Blade Components",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Chapman",
            "email": "achapman@spindogs.com"
        }
    ],
    "homepage": "https://github.com/spindogs/laravel-spin-forms",
    "keywords": ["Laravel", "LaravelSpinForms"],
    "require": {
        "illuminate/support": "~11|~12"
    },
    "require-dev": {
        "phpunit/phpunit": "~11"
    },
    "autoload": {
        "psr-4": {
            "Spindogs\\LaravelSpinForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spindogs\\LaravelSpinForms\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spindogs\\LaravelSpinForms\\SpinFormsServiceProvider"
            ]
        }
    }
}
